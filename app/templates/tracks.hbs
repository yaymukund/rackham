<table class="tracks">
  <tr>
    <th>Artist</th>
    <th>Title</th>
    <th>Album</th>
    <th>Uploader</th>
  </tr>

  {{#each queuedTracks}}
    <tr {{bind-attr class="hasPlayed isPlaying"}}>
      <td>{{artist}}</td>
      <td>{{title}}</td>
      <td>{{album}}</td>
      <td>{{user.login}}</td>
    </tr>
  {{/each}}
</table>
